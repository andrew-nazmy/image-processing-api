{
    "spec_dir": "dist/src",
  
    "spec_files": [
      "**/*[sS]pec.?(m)js",
      "!**/*nospec.js"
    ],
  
    "helpers": [
      "helpers/**/*.?(m)js"
    ],
    
    "reporters": [
      {
        "name": "jasmine-spec-reporter#spec_reporter",
        "options": {
          "displayStacktrace": "all"
        }
      }
    ],
    "env": {
   
      "failSpecWithNoExpectations": false,
      
      
      "stopSpecOnExpectationFailure": false,
  
      "stopOnSpecFailure": false,
 
      "random": false
    }
  }